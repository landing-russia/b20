{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "types": ["@astrojs/image/client"],
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@styles/*": ["src/styles/*"],
      "@assets/*": ["src/assets/*"],
      "@data/*": ["src/data/*"],
      "@types": ["src/types"],
      "@utils": ["src/utils"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
